{
  "version": 2,
  "name": "ibcol-dev",
  "alias": [
    "ibcol-dev.bbi.space"
  ],
  "regions": ["all"],
  "github": {
    "enabled": false,
    "autoAlias": false,
    "silent": true,
    "autoJobCancelation": true
  },
  "env": {
    "ENV": "dev",
    "NODE_ENV": "production"
  },
  "builds": [
    {
      "src": "next.config.js",
      "use": "@now/next"
    }
  ],
  "routes": [
      { "src": "/(?<locale>[^/]*)([/]*)$", "dest": "/home?locale=$locale", "headers": {"cache-control": "s-maxage=31536000, maxage=0"} },
      { "src": "/(?<locale>[^/]*)/how([/]*)$", "dest": "/how?locale=$locale", "headers": {"cache-control": "s-maxage=31536000, maxage=0"} },
      { "src": "/(?<locale>[^/]*)/ambassadors([/]*)$", "dest": "/ambassadors?locale=$locale", "headers": {"cache-control": "s-maxage=31536000, maxage=0"} },
      { "src": "/(?<locale>[^/]*)/sponsors([/]*)$", "dest": "/sponsors?locale=$locale", "headers": {"cache-control": "s-maxage=31536000, maxage=0"} },
      { "src": "/(?<locale>[^/]*)/contact([/]*)$", "dest": "/contact?locale=$locale", "headers": {"cache-control": "s-maxage=31536000, maxage=0"} },
      { "src": "/(?<locale>[^/]*)/registration([/]*)$", "dest": "/registration?locale=$locale", "headers": {"cache-control": "s-maxage=31536000, maxage=0"} },
      
      { "src": "/_next/static/(?:[^/]+/pages|chunks|runtime)/.+", "headers": { "cache-control": "immutable" } },
      {	
        "src": "^/service-worker.js$",	
        "dest": "/_next/static/service-worker.js",
        "headers": {
          "cache-control": "public, max-age=43200, immutable",
          "Service-Worker-Allowed": "/"
        }
      }
  ]
}