{
  "version": 2,
  "name": "ibcol-dev",
  "alias": [
    "ibcol-dev.bbi.codes"
  ],
  "regions": ["all"],
  "github": {
    "enabled": false,
    "autoAlias": false,
    "silent": true,
    "autoJobCancelation": true
  },
  "env": {
    "ENV": "dev",
    "NODE_ENV": "production"
  },
  "builds": [
    {
      "src": "next.config.js",
      "use": "@now/next"
    }
  ],
  "routes": [
    {	
      "src": "^/service-worker.js$",	
      "dest": "/_next/static/service-worker.js",
      "headers": {
        "cache-control": "public, max-age=43200, immutable",
        "Service-Worker-Allowed": "/"
      }
    }
  ]
}